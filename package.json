{
  "name": "testcafe",
  "version": "1.0.0",
  "description": "Curso TestCafe",
  "main": "index.js",
  "scripts": {
    "test:chrome": "testcafe chrome ./tests -s takeOnFails=true --disable-page-caching",
    "test:chrome:5": "testcafe -c 5 chrome ./tests -s takeOnfails=true",
    "test:safari": "testcafe safari ./tests -s takeOnFails=true --disable-page-caching",
    "test:firefox": "testcafe firefox ./tests -s takeOnFails=true --disable-page-caching",
    "test:chrome:headless": "testcafe chrome:headless ./tests -s takeOnFails=true --disable-page-caching",
    "test:chrome:mobile": "testcafe chrome:emulation:device=iphone X ./tests -s takeOnFails=true --disable-page-caching",
    "test:multiple": "testcafe chrome,safari ./tests -s takeOnfail=true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ccrj1105/CursoTestCafe.git"
  },
  "keywords": [
    "TestCafe",
    "Testing",
    "Automation"
  ],
  "author": "Cristian Rodriguez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ccrj1105/CursoTestCafe/issues"
  },
  "homepage": "https://github.com/ccrj1105/CursoTestCafe#readme",
  "dependencies": {
    "testcafe": "^1.14.2"
  },
  "devDependencies": {
    "xpath-to-css": "^1.0.5"
  }
}
